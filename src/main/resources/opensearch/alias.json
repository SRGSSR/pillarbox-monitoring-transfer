{
  "actions": [
    {
      "add": {
        "index": "actions-*",
        "alias": "filtered_actions",
        "filter": {
          "bool": {
            "must_not": [
              { "term": { "data.robot": true } },
              { "term": { "session.robot": true } }
            ]
          }
        }
      }
    }
  ]
}

