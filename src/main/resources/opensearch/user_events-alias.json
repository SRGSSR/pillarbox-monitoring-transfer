{
  "actions": [
    {
      "add": {
        "index": "core_events*",
        "alias": "user_events",
        "filter": {
          "bool": {
            "must_not": [
              { "term": { "session.robot": true } },
              { "term": { "session.embed": true } }
            ]
          }
        }
      }
    },
    {
      "add": {
        "index": "heartbeat_events*",
        "alias": "user_events",
        "filter": {
          "bool": {
            "must_not": [
              { "term": { "session.robot": true } },
              { "term": { "session.embed": true } }
            ]
          }
        }
      }
    }
  ]
}

